      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_3hTARhcpEeiw04JpNeJnEA" name="handleMessageFromServer">
        <language>JAVA</language>
        <body>&#xD;
    // If we received a String, figure out what this event is.&#xD;
    if (arg0 instanceof String) {&#xD;
        // Get the text of the message.&#xD;
        String message = (String) arg0;&#xD;
        // If we successfully logged in, tell the login controller.&#xD;
        if (message.equals(&quot;LoginSuccessful&quot;)) {&#xD;
            loginControl.loginSuccess();&#xD;
        } else // If we successfully created an account, tell the create account controller.&#xD;
        if (message.equals(&quot;CreateAccountSuccessful&quot;)) {&#xD;
            createAccountControl.createAccountSuccess();&#xD;
        }&#xD;
    } else // If we received an Error, figure out where to display it.&#xD;
    if (arg0 instanceof Error) {&#xD;
        // Get the Error object.&#xD;
        Error error = (Error) arg0;&#xD;
        // Display login errors using the login controller.&#xD;
        if (error.getType().equals(&quot;Login&quot;)) {&#xD;
            loginControl.displayError(error.getMessage());&#xD;
        } else // Display account creation errors using the create account controller.&#xD;
        if (error.getType().equals(&quot;CreateAccount&quot;)) {&#xD;
            createAccountControl.displayError(error.getMessage());&#xD;
        }&#xD;
    }&#xD;
</body>
      </ownedBehavior>
