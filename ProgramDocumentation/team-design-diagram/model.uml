      <nestedClassifier xmi:type="uml:Class" xmi:id="_3n1tJhcpEeiw04JpNeJnEA" name="EventHandler">
        <interfaceRealization xmi:type="uml:InterfaceRealization" xmi:id="_3n1tJxcpEeiw04JpNeJnEA" name="EventHandler implements ActionListener" client="_3n1tJhcpEeiw04JpNeJnEA" supplier="_3jrl4BcpEeiw04JpNeJnEA" contract="_3jrl4BcpEeiw04JpNeJnEA"/>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_3n1tKhcpEeiw04JpNeJnEA" name="actionPerformed" specification="_3n1tKBcpEeiw04JpNeJnEA">
          <language>JAVA</language>
          <body>&#xD;
    // Determine which button was clicked.&#xD;
    Object buttonClicked = e.getSource();&#xD;
    // Handle the Listen button.&#xD;
    if (buttonClicked == listen) {&#xD;
        // Display an error if the port number or timeout was not entered.&#xD;
        if (textFields[0].getText().equals(&quot;&quot;) || textFields[1].getText().equals(&quot;&quot;)) {&#xD;
            log.append(&quot;Port number or timeout not entered before pressing Listen\n&quot;);&#xD;
        } else // Otherwise, tell the server to start listening with the user's settings.&#xD;
        {&#xD;
            server.setPort(Integer.parseInt(textFields[0].getText()));&#xD;
            server.setTimeout(Integer.parseInt(textFields[1].getText()));&#xD;
            try {&#xD;
                server.listen();&#xD;
            } catch (IOException e1) {&#xD;
                log.append(&quot;An exception occurred: &quot; + e1.getMessage() + &quot;\n&quot;);&#xD;
            }&#xD;
        }&#xD;
    } else // Handle the Close button.&#xD;
    if (buttonClicked == close) {&#xD;
        // Display an error if the server has not been started.&#xD;
        if (!server.isRunning()) {&#xD;
            log.append(&quot;Server not currently started\n&quot;);&#xD;
        } else // Otherwise, close the server.&#xD;
        {&#xD;
            try {&#xD;
                server.close();&#xD;
            } catch (IOException e1) {&#xD;
                log.append(&quot;An exception occurred: &quot; + e1.getMessage() + &quot;\n&quot;);&#xD;
            }&#xD;
        }&#xD;
    } else // Handle the Stop button.&#xD;
    if (buttonClicked == stop) {&#xD;
        // Display an error if the server is not listening.&#xD;
        if (!server.isListening()) {&#xD;
            log.append(&quot;Server not currently listening\n&quot;);&#xD;
        } else // Otherwise, stop listening.&#xD;
        {&#xD;
            server.stopListening();&#xD;
        }&#xD;
    } else // For the Quit button, just stop this program.&#xD;
    if (buttonClicked == quit) {&#xD;
        System.exit(0);&#xD;
    }&#xD;
</body>
        </ownedBehavior>
